{
  "name": "tetrisgod",
  "version": "1.0.0",
  "description": "An in browser tetris game",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node server",
    "build": "tsc server/models/*.ts && cd react-ui/ && npm install && npm run build",
    "tsc": "tsc server/models/*.ts",
    "clean": "rm -rf server/models/*.js"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "express": "^4.16.4",
    "socket.io": "^2.2.0",
    "typescript": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cchoy96/tetrisgod.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {}
}
